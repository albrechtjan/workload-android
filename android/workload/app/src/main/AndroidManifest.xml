<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.gmail.konstantin.schubert.workload" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/MaterialTheme" >
        <activity
            android:name=".calendar"
            android:label="@string/title_activity_calendar" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service android:name=".AuthenticatorService" >
            <intent-filter>
                <action android:name="android.accounts.AccountAuthenticator" />
            </intent-filter>

            <meta-data
                android:name="android.accounts.AccountAuthenticator"
                android:resource="@xml/authenticator" />
        </service>

        <provider
            android:name=".SurveyContentProvider"
            android:authorities="de.tu-dresden.zqa.survey"
            android:exported="false"
            android:syncable="true" >
        </provider>

        <activity
            android:name=".SelectLecture"
            android:label="@string/title_activity_select_lecture"
            android:parentActivityName=".calendar" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.gmail.konstantin.schubert.workload.calendar" />
        </activity>
    </application>

</manifest>
